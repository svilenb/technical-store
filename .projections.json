{
  "routes/*.js": {
    "type": "route"
  },
  "views/*.js": {
    "type": "view"
  },
  "utils/*.js": {
    "type": "util"
  },
  "controllers/*.js": {
    "type": "controller"
  },
  "consts/*.js": {
    "type": "const"
  },
  "config/*.js": {
    "type": "config"
  },
  "data/*.js": {
    "type": "data"
  },
  "public/javascripts/*.js": {
    "type": "scripts"
  }
}
